{
  "name": "ftp-kr",
  "displayName": "ftp-kr",
  "description": "FTP/SFTP Sync Extension",
  "license": "MIT",
  "version": "1.2.11",
  "icon": "images/icon.png",
  "publisher": "ruakr",
  "repository": {
    "type": "git",
    "url": "https://github.com/karikera/ftp-kr"
  },
  "bugs": "https://github.com/karikera/ftp-kr/issues",
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.vscode/ftp-kr.json",
    "onCommand:ftpkr.init"
  ],
  "main": "./out/index",
  "contributes": {
    "commands": [
      {
        "command": "ftpkr.init",
        "title": "Init",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.cancel",
        "title": "Cancel All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.uploadAll",
        "title": "Upload All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.downloadAll",
        "title": "Download All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.upload",
        "title": "Upload This",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.download",
        "title": "Download This",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.refreshAll",
        "title": "Refresh All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.cleanAll",
        "title": "Clean All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.list",
        "title": "List",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.reconnect",
        "title": "Reconnect",
        "category": "ftp-kr"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "ftp-kr.json",
        "url": "./schema/ftp-kr.schema.json"
      },
      {
        "fileMatch": "make.json",
        "url": "./schema/make.schema.json"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ftpkr.upload",
          "title": "ftp-kr Upload",
          "group": "ftp-kr"
        },
        {
          "command": "ftpkr.download",
          "title": "ftp-kr Download",
          "group": "ftp-kr"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "mdgen": "node ./out/tool/schema_to_md.js ./schema/ftp-kr.schema.json"
  },
  "dependencies": {
    "ftp": "^0.3.10",
    "glob": "7.1.0",
	"iconv-lite": "0.4.13",
    "ssh2": "^0.5.5",
    "strip-json-comments": "2.0.1"
  },
  "devDependencies": {
    "@types/ftp": "^0.3.29",
    "@types/glob": "^5.0.34",
    "@types/iconv-lite": "0.0.1",
    "@types/mocha": "^2.2.46",
    "@types/node": "^6.0.96",
    "@types/ssh2": "^0.5.35",
    "@types/strip-json-comments": "0.0.28",
    "mocha": "^2.3.3",
    "typescript": "^2.6.2",
    "vscode": "^1.1.10"
  }
}
