{
  "name": "ftp-kr",
  "displayName": "ftp-kr",
  "description": "FTP sync & Closure Compiler for ME!",
  "license": "MIT",
  "version": "1.0.3",
  "publisher": "ruakr",
  "repository": {
    "type": "git",
    "url": "https://github.com/karikera/ftp-kr"
  },
  "bugs": "https://github.com/karikera/ftp-kr/issues",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.vscode/ftp-kr.json",
    "onCommand:ftpkr.init"
  ],
  "main": "./out/index",
  "contributes": {
    "commands": [
      {
        "command": "ftpkr.init",
        "title": "Init",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.makejson",
        "title": "Generate make.json",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.closureCompile",
        "title": "Closure Compile",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.closureCompileAll",
        "title": "Closure Compile All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.generateExtern",
        "title": "Closure Compiler Generate Extern",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.uploadAll",
        "title": "Upload All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.downloadAll",
        "title": "Download All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.upload",
        "title": "Upload This",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.download",
        "title": "Download This",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.refreshAll",
        "title": "Refresh All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.cleanAll",
        "title": "Clean All",
        "category": "ftp-kr"
      },
      {
        "command": "ftpkr.list",
        "title": "List",
        "category": "ftp-kr"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "ftp-kr.json",
        "url": "./schema/ftp-kr.schema.json"
      },
      {
        "fileMatch": "make.json",
        "url": "./schema/make.schema.json"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ftpkr.upload",
          "title": "ftp-kr Upload",
          "group": "ftp-kr"
        },
        {
          "command": "ftpkr.download",
          "title": "ftp-kr Download",
          "group": "ftp-kr"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "esprima": "^3.1.3",
    "ftp": "^0.3.10",
    "get-parameter-names": "^0.3.0",
    "glob": "7.1.0",
    "iconv-lite": "0.4.13",
    "ssh2-sftp-client": "^1.0.5",
    "strip-json-comments": "2.0.1"
  },
  "devDependencies": {
    "@types/ftp": "^0.3.29",
    "@types/glob": "^5.0.30",
    "@types/iconv-lite": "0.0.1",
    "@types/mocha": "^2.2.40",
    "@types/node": "^6.0.68",
    "@types/ssh2-sftp-client": "^1.0.5",
    "@types/strip-json-comments": "0.0.28",
    "mocha": "^2.3.3",
    "typescript": "^2.2.2",
    "vscode": "^1.1.0"
  }
}
